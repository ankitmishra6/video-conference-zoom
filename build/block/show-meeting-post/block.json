{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "vczapi/show-meeting-post",
  "version": "0.1.0",
  "title": "Embed Zoom Post",
  "category": "vczapi-blocks",
  "icon": "embed-post",
  "description": "Show a Meeting Post with Countdown",
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "gradients": true,
      "text": true,
      "link": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "postID": {
      "type": "number",
      "default": 0
    },
    "template": {
      "type": "string",
      "default": "none"
    },
    "description": {
      "type": "boolean",
      "default": true
    },
    "countdown": {
      "type": "boolean",
      "default": true
    },
    "details": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "video-conferencing-with-zoom-api",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php",
  "style": [
    "video-conferencing-with-zoom-api"
  ]
}