{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "vczapi/show-live-meeting",
  "version": "0.1.0",
  "title": "Direct Meeting or Webinar",
  "category": "vczapi-blocks",
  "icon": "sticky",
  "description": "Show a Meeting/Webinar details - direct from Zoom",
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "gradients": true,
      "text": true,
      "link": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "shouldShow": {
      "type": "object",
      "default": {
        "label": "Meeting",
        "value": "meeting"
      }
    },
    "host": {
      "type": "object"
    },
    "selectedMeeting": {
      "type": "object"
    },
    "link_only": {
      "type": "string",
      "default": "no"
    }
  },
  "textdomain": "show-live-meeting",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php"
}