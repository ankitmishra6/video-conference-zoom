{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "vczapi/recordings",
  "version": "1.0.0",
  "title": "Zoom - Show Recordings",
  "category": "vczapi-blocks",
  "icon": "playlist-video",
  "description": "Show a Meeting/Webinar details - direct from Zoom",
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "gradients": true,
      "text": true,
      "link": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "example": {},
  "attributes": {
    "shouldShow": {
      "type": "object",
      "default": {
        "label": "Meeting",
        "value": "meeting"
      }
    },
    "showBy": {
      "type": "string",
      "default": "host"
    },
    "host": {
      "type": "object"
    },
    "selectedMeeting": {
      "type": "object"
    },
    "downloadable": {
      "type": "string",
      "default": "no"
    }
  },
  "textdomain": "video-conferencing-with-zoom-api",
  "editorScript": "file:./index.js",
  "style": [
    "jquery-ui-datepicker-zvc",
    "video-conferencing-with-zoom-api-datable-responsive"
  ],
  "editorStyle": "file:./index.css",
  "render": "file:./render.php",
  "viewScript": [
    "jquery-ui-datepicker",
    "video-conferencing-with-zoom-api-datable-responsive-js",
    "video-conferencing-with-zoom-api-datable-dt-responsive-js",
    "video-conferencing-with-zoom-api-shortcode-js"
  ]
}