{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "vczapi/list-host-meetings",
	"version": "0.1.0",
	"title": "List Zoom Meetings by Host",
	"category": "vczapi-blocks",
	"icon": "list-view",
	"description": "Show Meetings/Webinars by Host",
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"gradients": true,
			"text": true,
			"link": true
		},
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"textdomain": "video-conferencing-with-zoom-api",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": ["video-conferencing-with-zoom-api"],
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"attributes": {
		"host": {
			"type": "object"
		},
		"shouldShow": {
			"type": "object",
			"default": {
				"label": "Meeting",
				"value": "meeting"
			}
		},
		"preview": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {
		"attributes": {
			"preview": true
		}
	}
}